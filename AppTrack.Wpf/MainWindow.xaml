<Window x:Class="AppTrack.Wpf.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AppTrack.Wpf"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel Margin="10">
        <Label Content="AppTrack" FontSize="20"></Label>
        <DataGrid>
            <DataGrid.Columns>
                <DataGridTextColumn Header="ID"></DataGridTextColumn>
                <DataGridTextColumn Header="Client Name"></DataGridTextColumn>
                <DataGridTextColumn Header="Position"></DataGridTextColumn>
                <DataGridTextColumn Header="URL"></DataGridTextColumn>
                <DataGridTextColumn Header="Status"></DataGridTextColumn>
                <DataGridTextColumn Header="Application Text"></DataGridTextColumn>
                <DataGridTextColumn Header="Created"></DataGridTextColumn>
                <DataGridTextColumn Header="Modified"></DataGridTextColumn>
                <DataGridTextColumn Header="Functions"></DataGridTextColumn>
            </DataGrid.Columns>
        </DataGrid>
    </StackPanel>
</Window>
